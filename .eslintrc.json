{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:react/recommended",
    "airbnb",
    "plugin:prettier/recommended",
    "prettier"
    ],
    "parserOptions": {
      "ecmaFeatures": {
          "jsx": true,
          "tsx": true,
          "ts"
